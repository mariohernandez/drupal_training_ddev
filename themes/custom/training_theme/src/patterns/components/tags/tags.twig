{{ attach_library('training_theme/tags') }}

<ul class="tags{{- attributes ? ' ' ~ attributes.class -}}"
  {{- attributes ? attributes|without(class) -}}>
  {% for item in items %}
    {% block tag_item %}
      {%
        include '@training_theme/tags/_tag-item.twig' with {
          "name": item.name,
          "url": item.url
        } only
      %}
    {% endblock %}
  {% endfor %}
</ul>
